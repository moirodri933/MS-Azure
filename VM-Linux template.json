{
    "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "virtualMachines_VM_Linux_test01_name": {
            "defaultValue": "VM-Linux-test01",
            "type": "String"
        },
        "disks_VM_Linux_test01_disk1_e6c63fad8ddd46d6915d3371044f14c5_externalid": {
            "defaultValue": "/subscriptions/0509a3a9-0809-4f61-9131-b2309f9af23b/resourceGroups/AZ1000301-MSA/providers/Microsoft.Compute/disks/VM-Linux-test01_disk1_e6c63fad8ddd46d6915d3371044f14c5",
            "type": "String"
        },
        "networkInterfaces_vm_linux_test01772_externalid": {
            "defaultValue": "/subscriptions/0509a3a9-0809-4f61-9131-b2309f9af23b/resourceGroups/az1000301-MSA/providers/Microsoft.Network/networkInterfaces/vm-linux-test01772",
            "type": "String"
        }
    },
    "variables": {},
    "resources": [
        {
            "type": "Microsoft.Compute/virtualMachines",
            "apiVersion": "2019-07-01",
            "name": "[parameters('virtualMachines_VM_Linux_test01_name')]",
            "location": "eastus",
            "properties": {
                "hardwareProfile": {
                    "vmSize": "Standard_D2s_v3"
                },
                "storageProfile": {
                    "imageReference": {
                        "publisher": "Canonical",
                        "offer": "UbuntuServer",
                        "sku": "18.04-LTS",
                        "version": "latest"
                    },
                    "osDisk": {
                        "osType": "Linux",
                        "name": "[concat(parameters('virtualMachines_VM_Linux_test01_name'), '_disk1_e6c63fad8ddd46d6915d3371044f14c5')]",
                        "createOption": "FromImage",
                        "caching": "ReadWrite",
                        "managedDisk": {
                            "id": "[parameters('disks_VM_Linux_test01_disk1_e6c63fad8ddd46d6915d3371044f14c5_externalid')]"
                        }
                    },
                    "dataDisks": []
                },
                "osProfile": {
                    "computerName": "[parameters('virtualMachines_VM_Linux_test01_name')]",
                    "adminUsername": "test-lab",
                    "linuxConfiguration": {
                        "disablePasswordAuthentication": false,
                        "provisionVMAgent": true
                    },
                    "secrets": [],
                    "allowExtensionOperations": true,
                    "requireGuestProvisionSignal": true
                },
                "networkProfile": {
                    "networkInterfaces": [
                        {
                            "id": "[parameters('networkInterfaces_vm_linux_test01772_externalid')]"
                        }
                    ]
                }
            }
        }
    ]
}


